{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "outDir": "./js/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true
  },
  "exclude": [
    "node_modules",
    "typings/globals",
    "tests"
  ]
}